{
    "name": "Undead",
    "description": "Hostile mobs will not attack you, but you cannot trade with villagers.",
    "type": "origins:multiple",
    "nohostile": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:area_of_effect",
            "radius": 32767,
            "bientity_condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:in_tag",
                            "tag": "msmp_origins:undead"
                        },
                        {
                            "type": "origins:command",
                            "command": "execute unless entity @s[team=msmp_origins.undead]",
                            "comparison": ">=",
                            "compare_to": 1
                        }
                    ]
                }
            },
            "bientity_action": {
                "type": "origins:target_action",
                "action": {
                    "type": "origins:execute_command",
                    "command": "team join msmp_origins.undead @s"
                }
            }
        }
    },
    "notrade": {
        "type": "origins:prevent_entity_use",
        "bientity_condition": {
            "type": "origins:target_condition",
            "condition": {
                "type": "origins:entity_type",
                "entity_type": "minecraft:villager"
            }
        }
    },
    "teamdef": {
        "type": "origins:action_on_callback",
        "entity_action_chosen": {
            "type": "origins:execute_command",
            "command": "team add msmp_origins.undead"
        },
        "execute_chosen_when_orb": true,
        "entity_action_lost": {
            "type": "origins:execute_command",
            "command": "team leave @s"
        },
        "entity_action_added": {
            "type": "origins:execute_command",
            "command": "team join msmp_origins.undead @s"
        }
    }
}